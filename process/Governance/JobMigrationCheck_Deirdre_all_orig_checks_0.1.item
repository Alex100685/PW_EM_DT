    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#13;&#10;INSERT INTO mycomments&#13;&#10;&#13;&#10;WITH hasnochild AS (&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;ticketnumber,&#13;&#10;&#9;&#9;projectname,&#13;&#10;&#9;&#9;cleanedjobname&#9;&#13;&#10;&#9;FROM&#13;&#10;&#9;&#9;basetable&#13;&#10;&#9;GROUP BY&#13;&#10;&#9;&#9;ticketnumber,&#13;&#10;&#9;&#9;projectname,&#13;&#10;&#9;&#9;cleanedjobname&#13;&#10;&#9;HAVING max(CASE WHEN lower(componentname) LIKE 'trunjob%' THEN 1 ELSE 0 END) = 0&#13;&#10;), distinctcontextlogins AS (&#13;&#10;&#9;SELECT DISTINCT&#13;&#10;&#9;&#9;base.ticketnumber,&#13;&#10;&#9;&#9;base.projectname,&#13;&#10;&#9;&#9;substring(base.jobname, 1, length(base.jobname) - position('_' IN &quot; + context.jobcheck_db_Schema + &quot;.reverse(base.jobname))) AS cleanedjobname,&#13;&#10;&#9;&#9;base.contextname,&#13;&#10;&#9;&#9;'context.' || base.contextname AS realcontextname&#13;&#10;&#9;FROM&#13;&#10;&#9;&#9;&quot; + context.jobcheck_db_Schema + &quot;.xml_contexts_&quot; + globalMap.get(&quot;USERNAME&quot;) + &quot; base&#13;&#10;&#9;&#9;JOIN LatestRevision rev ON&#13;&#10;&#9;&#9;&#9;base.ticketnumber = rev.ticketnumber&#13;&#10;&#9;&#9;&#9;AND base.projectname = rev.projectname&#13;&#10;&#9;&#9;&#9;AND base.jobname = rev.origjobname&#13;&#10;&#9;WHERE&#13;&#10;&#9;&#9;lower(base.contextname) LIKE '%user' OR lower(base.contextname) LIKE '%login'&#13;&#10;), hardcoded_logins AS (&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;base.ticketnumber,&#13;&#10;&#9;&#9;base.projectname,&#13;&#10;&#9;&#9;base.cleanedjobname,&#13;&#10;&#9;&#9;'CONTEXT'::VARCHAR(100) AS category,&#13;&#10;&#9;&#9;'WARNING'::VARCHAR(100) mycommenttype,&#13;&#10;        'The Login/User seems to be a hardcoded value instead of a context variable!'::TEXT AS mycomment,&#13;&#10;&#9;&#9;CASE WHEN count(DISTINCT base.componentname) &lt; 10 THEN string_agg(DISTINCT base.componentname, ';') ELSE 'More then 10 components are affected!' END::text AS componentlist&#13;&#10;&#9;FR