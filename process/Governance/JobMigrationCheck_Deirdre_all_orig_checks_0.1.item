    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#13;&#10;CREATE temp table HighLevelCheck AS (&#13;&#10;&#9;&#13;&#10;&#9;WITH simplecomponents AS (&#13;&#10;&#9;SELECT DISTINCT &#13;&#10;&#9;&#9;base.ticketnumber,&#13;&#10;&#9;&#9;base.projectname,&#13;&#10;&#9;&#9;base.cleanedjobname AS jobname,&#13;&#10;&#9;&#9;substring(base.componentname, 1, length(base.componentname) - position('_' IN &quot; + context.jobcheck_db_Schema + &quot;.reverse(base.componentname))) AS componentname&#13;&#10;&#9;&#13;&#10;&#9;FROM &#13;&#10;&#9;&#9;basetable base&#13;&#10;&#9;), basedata AS (&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;ticketnumber,&#13;&#10;&#9;&#9;projectname,&#13;&#10;&#9;&#9;jobname,&#13;&#10;&#9;&#9;max(CASE &#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tas400%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'taccess%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tamazon%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tcassandra%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tcouch%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tdb%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tjdb%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'texa%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tfirebird%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'thbase%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'thsql%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'thive%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tinformix%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tingres%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tinterbase%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tjavadb%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tldap%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tmssql%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tmaxdb%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tmongodb%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tmysql%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tneo4j%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tnetezza%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'toledb%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'toracle%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tparaccel%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tpostgres%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tredshift%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'triak%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tsap%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tsqlite%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tsas%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tsybase%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tteradata%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tvectorwise%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'tvertica%' then 1&#13;&#10;&#9;&#9;&#9;when lower(componentname) like 'texist%' then 1&#13;&#10;&#9;&#9;ELSE 0 END) AS hasotherdb_than_gpcomp,&#13;&#10;&#9;&#9;max(CASE WHEN componentname LIKE 'tGreenplum%' THEN 1 ELSE 0 END) AS connects_gp,&#13;&#10;&#9;&#9;max(CASE WHEN componentname LIKE 'tFileInput%' THEN 1 ELSE 0 END) AS has_fileinput,&#13;&#10;&#9;&#9;max(CASE WHEN componentname LIKE 'tFileOutput%' THEN 1 ELSE 0 END) AS has_fileoutput,&#13;&#10;&#9;&#9;max(CASE WHEN componentname LIKE 'tGreenplumInput%' THEN 1 ELSE 0 END) AS has_gpinput,&#13;&#10;&#9;&#9;max(CASE WHEN componentname LIKE 'tGreenplumOutput%' THEN 1 ELSE 0 END) AS has_gpoutput,&#13;&#10;&#9;&#9;max(CASE WHEN componentname LIKE 'tSetGlobalVar%' OR componentname LIKE 'tJava%' THEN 1 ELSE 0 END) AS has_variable&#13;&#10;&#9;FROM&#13;&#10;&#9;&#9;simplecomponents&#13;&#10;&#9;GROUP BY&#13;&#10;&#9;&#9;ticketnumber,&#13;&#10;&#9;&#9;projectname,&#13;&#10;&#9;&#9;jobname&#13;&#10;&#9;)&#13;&#10;&#9;&#13;&#10;&#9;SELECT &#13;&#10;&#9;&#9;basedata.ticketnumber,&#13;&#10;&#9;&#9;basedata.projectname,&#13;&#10;&#9;&#9;basedata.jobname,&#13;&#10;&#9;&#9;basedata.hasotherdb_than_gpcomp,&#13;&#10;&#9;&#9;basedata.connects_gp,&#13;&#10;&#9;&#9;basedata.has_fileinput,&#13;&#10;&#9;&#9;basedata.has_fileoutput,&#13;&#10;&#9;&#9;basedata.has_gpinput,&#13;&#10;&#9;&#9;basedata.has_gpoutput,&#13;&#10;&#9;&#9;basedata.has_variable,&#13;&#10;&#9;&#9;coalesce(distinctusers.cnt, 0) AS distinct_gpuser_count,&#13;&#10;&#9;&#9;sum(CASE WHEN gpload.componentname LIKE '%GPLoad%' THEN 1 ELSE 0 END) AS gpload_count&#13;&#10;&#9;FROM&#13;&#10;&#9;&#9;basedata basedata&#13;&#10;&#9;&#9;LEFT JOIN (&#13;&#10;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;base.ticketnumber,&#13;&#10;&#9;&#9;&#9;&#9;base.projectname,&#13;&#10;&#9;&#9;&#9;&#9;base.cleanedjobname,&#13;&#10;&#9;&#9;&#9;&#9;count(DISTINCT con.contextvalue) AS cnt&#13;&#10;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;basetable base&#13;&#10;&#9;&#9;&#9;&#9;LEFT JOIN BaseTable rel ON&#13;&#10;&#9;&#9;&#9;&#9;&#9;base.ticketnumber = rel.ticketnumber&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND base.projectname = rel.projectname&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND base.jobname = rel.jobname&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND rel.componentname like 'tGreenplumConnection%'&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND base.connection = rel.componentname&#13;&#10;&#9;&#9;&#9;&#9;LEFT JOIN &quot; + context.jobcheck_db_Schema + &quot;.xml_contexts_&quot; + globalMap.get(&quot;USERNAME&quot;) + &quot; con ON&#13;&#10;&#9;&#9;&#9;&#9;&#9;base.ticketnumber = con.ticketnumber&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND base.projectname = con.projectname&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND base.jobname = con.jobname&#13;&#10;&#9;&#9;&#9;&#9;&#9;AND 'context.' || con.contextname = base.us