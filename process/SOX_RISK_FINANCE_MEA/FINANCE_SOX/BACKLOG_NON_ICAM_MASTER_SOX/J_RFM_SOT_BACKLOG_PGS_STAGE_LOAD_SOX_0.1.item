    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;insert into SOT_PGSBACKLOG.FS_BACKLOG_STG&#13;&#10;SELECT FS_UNIQUE_ID,&#13;&#10;SOURCE_DATA,&#13;&#10;CUST_NM,&#13;&#10;CUST_TYPE_NM,&#13;&#10;CUST_SHIPPED_TO,&#13;&#10;CUST_ADDR_CNTRY_NM,&#13;&#10;CUST_ADDR_STATE_NM,&#13;&#10;PROJ_CLSS_DESC,&#13;&#10;PROJ_CLSS_CTGRY_DESC,&#13;&#10;PROJ_TYPE_NM,&#13;&#10;PROJ_STAT_VAL,&#13;&#10;OU,&#13;&#10;EMPLY_FULL_NM,&#13;&#10;ORG_NM,&#13;&#10;CRRCY_CD,&#13;&#10;PROJ_CRRCY_CD,&#13;&#10;TASK_SRVC_OFRING_NUM,&#13;&#10;PROJ_EVNT_ID,&#13;&#10;PROJ_PL_NM,&#13;&#10;PROJ_OWNG_ORG_NM,&#13;&#10;PROJ_CLSS_SGMNT,&#13;&#10;TASK_EQUIP_SL_NUM,&#13;&#10;TASK_TCHNLGY_CD,&#13;&#10;TASK_EQUIP_CD,&#13;&#10;PROJ_SRVC_MGR_NM,&#13;&#10;PROJ_OUTG_END_DT,&#13;&#10;PROJ_NUM,&#13;&#10;substr(PROJ_LE_ME_VAL,instr(PROJ_LE_ME_VAL,'_',1,1)+1) ME,&#13;&#10;TASK_NUM,&#13;&#10;PROJ_DESC,&#13;&#10;TASK_DESC,&#13;&#10;BACKLOG_TYPE,&#13;&#10;FUNC_CURR_TOT_ORDERS_RECV,    --IMP&#13;&#10;AGMT_NUM,&#13;&#10;AGMT_DT,&#13;&#10;CUR_SALES,&#13;&#10;FUNC_CURR_SALES_TO_DATE,   --IMP&#13;&#10;FUNC_CURR_BACKLOG_AMT,   --IMP&#13;&#10;FUNDING_CURR_TOT_ORDERS_RECV,&#13;&#10;FUNDING_CURR_SALES_TO_DATE,&#13;&#10;FUNDING_CURR_BACKLOG_AMT,&#13;&#10;BILL_PERIOD_FUNCTIONAL,&#13;&#10;BUDGET_FORCST_SALES_AMT,&#13;&#10;YYYYMMDD,&#13;&#10;CALENDAR_FISCL_YR_MO_NUM,&#13;&#10;FNDG_CRRCY,&#13;&#10;FUNC_CURR_BACKLOG_AMT*COALESCE(PRCTG,1) Allocated_Backlog,   --IMP&#13;&#10;FUNC_CURR_SALES_TO_DATE*COALESCE(PRCTG,1) Allocated_Sales,   --IMP&#13;&#10;FUNC_CURR_TOT_ORDERS_RECV*COALESCE(PRCTG,1) Allocated_Order   --IMP&#13;&#10;from&#13;&#10;(&#13;&#10;SELECT &#13;&#10;1 FS_UNIQUE_ID,&#13;&#10;'FS' SOURCE_DATA,&#13;&#10;A.* &#13;&#10;FROM &#13;&#10;(&#13;&#10;SELECT &#13;&#10;DISTINCT &#13;&#10;  OTHER_DIM_ATTRIBUTES.CUST_NM, &#13;&#10;  OTHER_DIM_ATTRIBUTES.CUST_TYPE_NM, &#13;&#10;  OTHER_DIM_ATTRIBUTES.CUSTOMER_SHIPPED_TO_NAME CUST_SHIPPED_TO, &#13;&#10;  OTHER_DIM_ATTRIBUTES.CUST_ADDR_CNTRY_NM, &#13;&#10;  OTHER_DIM_ATTRIBUTES.CUST_ADDR_STATE_NM, &#13;&#10;  OTHER_DIM_ATTRIBUTES.proj_clss_desc, &#13;&#10;  FACT_VALUES.PROJ_CLSS_CTGRY_DESC PROJ_CLSS_CTGRY_DESC, &#13;&#10;  NULL SERVICE_TYPE, &#13;&#10;  OTHER_DIM_ATTRIBUTES.PROJ_STAT_VAL, &#13;&#10;  null LOC_RGN_NM, &#13;&#10;  null LOC_STR03_LVL_5_DESC, &#13;&#10;  null LOC_STR20_lvl_3_DESC, &#13;&#10;  null LOC_CNTRY_NM, &#13;&#10;  null LOC_STR03_LVL_6_DESC, &#13;&#10;  null LOC_STR20_LVL_4_DESC, &#13;&#10;  null LOC_ME_CD, &#13;&#10;  FACT_VALUES.PROJECT_TYPE PROJ_TYPE_NM, &#13;&#10;  OTHER_DIM_ATTRIBUTES.ORG_NM OU, &#13;&#10;  OTHER_DIM_ATTRIBUTES.EMPLY_FULL_NM, &#13;&#10;  OU1.NAME ORG_NM, &#13;&#10;  null CRRCY_CD, &#13;&#10;  OTHER_DIM_ATTRIBUTES.PROJ_CRRCY_CD, &#13;&#10;  FACT_VALUES.TASK_SRVC_OFRING_NUM TASK_SRVC_OFRING_NUM, &#13;&#10;  OTHER_DIM_ATTRIBUTES.PROJ_EVNT_ID, &#13;&#10;  OTHER_DIM_ATTRIBUTES.PROJ_PL_NM, &#13;&#10;  NULL PROJ_OWNG_ORG_NM, &#13;&#10;  FACT_VALUES.PROJ_CLSS_SGMNT, &#13;&#10;  FACT_VALUES.TASK_EQUIP_SL_NUM, &#13;&#10;  FACT_VALUES.TASK_TCHNLGY_CD, &#13;&#10;  FACT_VALUES.TASK_EQUIP_CD, &#13;&#10;  FACT_VALUES.PROJ_SRVC_MGR_NM, &#13;&#10;  FACT_VALUES.PROJ_OUTG_END_DT, &#13;&#10;  FACT_VALUES.PROJ_NUM, &#13;&#10;  FACT_VALUES.PROJ_LE_ME_VAL, &#13;&#10;  FACT_VALUES.TASK_NUM, &#13;&#10;  FACT_VALUES.PROJ_DESC, &#13;&#10;  FACT_VALUES.TASK_DESC, &#13;&#10;  FACT_VALUES.BACKLOG_TYPE, &#13;&#10;  FACT_VALUES.FUNC_CURR_TOT_ORDERS_RECV, &#13;&#10;  FACT_VALUES.AGMT_NUM, &#13;&#10;  FACT_VALUES.AGMT_DT, &#13;&#10;  0 CUR_SALES, &#13;&#10;  FACT_VALUES.FUNC_CURR_SALES_TO_DATE, &#13;&#10;  FACT_VALUES.FUNC_CURR_BACKLOG_AMT,&#13;&#10;  0 FUNDING_CURR_TOT_ORDERS_RECV,&#13;&#10;  0 FUNDING_CURR_SALES_TO_DATE, &#13;&#10;  0 FUNDING_CURR_BACKLOG_AMT, &#13;&#10;  0 BILL_PERIOD_FUNCTIONAL,&#13;&#10;  BUDGET_PCTG.BUDGET_FORCST_SALES_AMT , &#13;&#10;COALESCE(SUBSTR(BUDGET_PCTG.CALENDAR_FISCL_YR_MO_NUM,1,4),&#13;&#10;  TO_CHAR(CURRENT_DATE,'YYYY'))||'-'&#13;&#10;  ||COALESCE(SUBSTR(BUDGET_PCTG.CALENDAR_FISCL_YR_MO_NUM,5,2),&#13;&#10;  TO_CHAR(CURRENT_DATE,'MM'))||'-05' AS YYYYMMDD,&#13;&#10;  CAST(BUDGET_PCTG.CALENDAR_FISCL_YR_MO_NUM AS NUMERIC) CALENDAR_FISCL_YR_MO_NUM,&#13;&#10; cast(TO_CHAR(current_date,'YYYYMM') as numeric(15,0)) CUR_YR_MN,&#13;&#10;  null FNDG_CRRCY,&#13;&#10; BUDGET_PCTG.PRCTG &#13;&#10;FROM &#13;&#10;(&#13;&#10;SELECT DISTINCT &#13;&#10;    PROJ_ALL.SEGMENT1 PROJ_NUM,&#13;&#10;    BILL_FINAL.PROJECT_ID ,&#13;&#10;    BILL_FINAL.TASK_ID,&#13;&#10;    PROJ_LE_ME_VAL.PROJ_LE_ME PROJ_LE_ME_VAL,&#13;&#10;    PTSK.TASK_NUMBER TASK_NUM,&#13;&#10;    BILL_FINAL.AGMT_NUM,&#13;&#10;    BILL_FINAL.AGMT_DT,&#13;&#10;    BILL_FINAL.PROJ_CLSS_CTGRY_DESC,&#13;&#10;    PROJ_ALL.DESCRIPTION PROJ_DESC,&#13;&#10;    PTSK.DESCRIPTION TASK_DESC,&#13;&#10;    (CASE&#13;&#10;        WHEN UPPER(PROJ_ALL.SEGMENT1) IN ('10189605')&#13;&#10;        THEN 'R'&#13;&#10;        WHEN UPPER(TYPES_ALL.ATTRIBUTE7) = 'RECEIVABLES'&#13;&#10;        THEN 'F'&#13;&#10;        WHEN UPPER(TYPES_ALL.ATTRIBUTE7) = 'REVENUE'&#13;&#10;        THEN 'F'&#13;&#10;        WHEN UPPER(TYPES_ALL.ATTRIBUTE7) = 'PROJECTS'&#13;&#10;        THEN 'R'&#13;&#10;    END) BACKLOG_TYPE,&#13;&#10;    BILL_FINAL.FUNC_CURR_SALES_TO_DATE,&#13;&#10;    PROJ_CLS.CLASS_CODE PROJ_CLSS_SGMNT,&#13;&#10;    PTSK.ATTRIBUTE1 TASK_EQUIP_SL_NUM,&#13;&#10;    PTSK.ATTRIBUTE2 TASK_TCHNLGY_CD,&#13;&#10;    PTSK.ATTRIBUTE3 TASK_EQUIP_CD,&#13;&#10;    PTSK.COMPLETION_DATE PROJ_OUTG_END_DT,&#13;&#10;    PROJ_SRVC_MGR.FULL_NAME PROJ_SRVC_MGR_NM,&#13;&#10;    BILL_FINAL.PROJFUNC_ALLOCATED_AMOUNT FUNC_CURR_TOT_ORDERS_RECV,&#13;&#10;    (BILL_FINAL.PROJFUNC_ALLOCATED_AMOUNT- BILL_FINAL.FUNC_CURR_SALES_TO_DATE) &#10; FUNC_CURR_BACKLOG_AMT,   &#13;&#10;    PTSK.ATTRIBUTE6 TASK_SRVC_OFRING_NUM,&#13;&#10;    BILL_FINAL.PROJECT_TYPE&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT COALESCE(bill_main2.func_curr_sales_to_date, 0) func_curr_sales_to_date, &#13;&#10;      COALESCE(bill_main2.bill_period_functional_bill, 0) bill_period_functional_bill ,&#13;&#10;      PROJ_FUND.* ,&#13;&#10;      BILL_MAIN2.PROJECT_TYPE &#13;&#10;    FROM &#13;&#10;    (&#13;&#10;SELECT &#13;&#10;FUND.PROJECT_ID,&#13;&#10;FUND.TASK_ID TASK_ID,&#13;&#10;MAX(AGMT.ATTRIBUTE9) PROJ_CLSS_CTGRY_DESC,&#13;&#10;MAX(AGMT.AGREEMENT_NUM)                    AS AGMT_NUM,&#13;&#10;MAX(AGMT.CREATION_DATE)                    AS AGMT_DT,&#13;&#10;SUM(COALESCE(PROJFUNC_ALLOCATED_AMOUNT,0)) AS PROJFUNC_ALLOCATED_AMOUNT&#13;&#10;FROM &#13;&#10;  (SELECT PROJECT_ID,SEGMENT1,PROJECT_TYPE,ORG_ID &#13;&#10;  FROM PGSCHARLIE.PA_PROJECTS_ALL-- WHERE SEGMENT1 = '10186211'&#13;&#10;  ) PROJ_ALL&#13;&#10;  JOIN&#9;&#13;&#10;  (SELECT PROJECT_ID,TASK_ID,PROJFUNC_ALLOCATED_AMOUNT,AGREEMENT_ID &#13;&#10;  FROM PGSCHARLIE.PA_PROJECT_FUNDINGS)          AS FUND&#13;&#10;  ON&#13;&#10;  PROJ_ALL.PROJECT_ID = FUND.PROJECT_ID&#13;&#10;  LEFT OUTER JOIN &#13;&#10;(SELECT AGREEMENT_ID,ATTRIBUTE9,AGREEMENT_NUM,CREATION_DATE &#13;&#10;FROM PGSCHARLIE.PA_AGREEMENTS_ALL) AS AGMT&#13;&#10;ON (FUND.AGREEMENT_ID = AGMT.AGREEMENT_ID)&#13;&#10;GROUP BY &#13;&#10;FUND.PROJECT_ID, FUND.TASK_ID&#13;&#10;) PROJ_FUND&#13;&#10;LEFT JOIN&#13;&#10;      (&#13;&#10;      SELECT bill_main.*, &#13;&#10;        bill_select_period.bill_period_functional_bill &#13;&#10;      FROM &#13;&#10;        (&#13;&#10;SELECT DISTINCT AA.PROJECT_ID,&#13;&#10;        AA.TASK_ID task_id,&#13;&#10;        AA.TASK_NUMBER,&#13;&#10;        AA.PROJECT_TYPE,&#13;&#10;        SUM(COALESCE(AA.BLNG_FUNCNL_BILL_AMT,0)) FUNC_CURR_SALES_TO_DATE&#13;&#10;      FROM&#13;&#10;        (&#13;&#10;&#9;SELECT &#13;&#10;        I.PROJECT_ID PROJECT_ID,&#13;&#10;        I.TASK_ID TASK_ID,&#13;&#10;        PTSK.TASK_NUMBER,&#13;&#10;        PROJ_TYPES.PROJECT_TYPE,&#13;&#10;        SUM(I.PROJFUNC_BILL_AMOUNT) BLNG_FUNCNL_BILL_AMT&#13;&#10;  FROM &#13;&#10;  (select ACCTD_CURR_CODE,INV_CURRENCY_CODE,INVOICE_DATE,AGREEMENT_ID,&#13;&#10;  RA_INVOICE_NUMBER,ACCTD_EXCHG_RATE,TRANSFER_STATUS_CODE,&#13;&#10;  SYSTEM_REFERENCE,DRAFT_INVOICE_NUM,DRAFT_INVOICE_NUM_CREDITED,&#13;&#10;  CREATION_DATE,GENERATION_ERROR_FLAG,TRANSFER_REJECTION_REASON,&#13;&#10;  GL_DATE,BILL_THROUGH_DATE,PROJECT_ID,unearned_code_combination_id,&#13;&#10;  unbilled_code_combination_id from pgscharlie.PA_DRAFT_INVOICES_ALL&#13;&#10;  WHERE UPPER(TRANSFER_STATUS_CODE) = 'A') IA&#13;&#10;  join &#13;&#10;  (select PROJECT_ID,TASK_ID,LINE_NUM,TEXT,INV_AMOUNT,EVENT_NUM,&#13;&#10;  FUNDING_BILL_AMOUNT,PROJFUNC_BILL_AMOUNT,DRAFT_INVOICE_NUM &#13;&#10;  from pgscharlie.PA_DRAFT_INVOICE_ITEMS) I&#13;&#10;  on IA.PROJECT_ID = I.PROJECT_ID&#13;&#10;  and IA.DRAFT_INVOICE_NUM = I.DRAFT_INVOICE_NUM&#13;&#10;  join&#13;&#10;  (select segment2,code_combination_id from pgscharlie.gl_code_combinations) gl&#13;&#10;  on coalesce(IA.unearned_code_combination_id,&#10; IA.unbilled_code_combination_id) =  gl.code_combination_id&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,SEGMENT1,PROJECT_TYPE,ORG_ID FROM PGSCHARLIE.PA_PROJECTS_ALL &#13;&#10;  --WHERE SEGMENT1 = '10186211'&#13;&#10;  ) PROJ_ALL&#13;&#10;  ON (IA.PROJECT_ID = PROJ_ALL.PROJECT_ID)&#9;&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_TYPE,ORG_ID FROM PGSCHARLIE.PA_PROJECT_TYPES_ALL &#13;&#10;  WHERE UPPER(ATTRIBUTE10) = 'N') PROJ_TYPES&#13;&#10;  ON (PROJ_ALL.PROJECT_TYPE = PROJ_TYPES.PROJECT_TYPE&#13;&#10;  AND PROJ_ALL.ORG_ID       = PROJ_TYPES.ORG_ID)&#13;&#10;  JOIN&#13;&#10;  (SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS &#13;&#10;  WHERE UPPER(NAME) LIKE 'OU%') OU&#13;&#10;  ON&#13;&#10;  PROJ_ALL.ORG_ID = OU.ORGANIZATION_ID &#13;&#10;  JOIN&#13;&#10;  (SELECT TASK_ID,TASK_NUMBER FROM PGSCHARLIE.PA_TASKS) PTSK&#13;&#10;  ON I.TASK_ID = PTSK.TASK_ID&#13;&#10;  GROUP BY &#13;&#10;  I.PROJECT_ID,&#13;&#10;  I.TASK_ID,&#13;&#10;  PTSK.TASK_NUMBER,&#13;&#10;  PROJ_TYPES.PROJECT_TYPE&#13;&#10;  UNION&#13;&#10;  SELECT &#13;&#10;  REV.PROJECT_ID,&#13;&#10;  REV.TASK_ID,&#13;&#10;  PTSK.TASK_NUMBER,&#13;&#10;  PROJ_TYPES.PROJECT_TYPE,&#13;&#10;  SUM(REV.PROJFUNC_REVENUE_AMOUNT) BLNG_FUNCNL_BILL_AMT&#13;&#10;  FROM &#13;&#10;  (SELECT PROJECT_ID,TASK_ID,DRAFT_REVENUE_NUM,PROJFUNC_REVENUE_AMOUNT &#13;&#10;  FROM PGSCHARLIE.PA_DRAFT_REVENUE_ITEMS&#13;&#10;   WHERE UPPER(REVENUE_SOURCE) &lt;&gt; 'DUMMY') REV&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,DRAFT_REVENUE_NUM FROM PGSCHARLIE.PA_DRAFT_REVENUES_ALL &#13;&#10;  WHERE UPPER(TRANSFER_STATUS_CODE) = 'A') REV_ALL&#13;&#10;  ON (REV.PROJECT_ID        = REV_ALL.PROJECT_ID&#13;&#10;  AND REV.DRAFT_REVENUE_NUM = REV_ALL.DRAFT_REVENUE_NUM)&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,ORG_ID,PROJECT_TYPE FROM PGSCHARLIE.PA_PROJECTS_ALL &#13;&#10;  ) PROJ_ALL&#13;&#10;  ON (REV.PROJECT_ID = PROJ_ALL.PROJECT_ID)&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_TYPE,ORG_ID FROM PGSCHARLIE.PA_PROJECT_TYPES_ALL &#13;&#10;  WHERE UPPER(ATTRIBUTE10) = 'Y') PROJ_TYPES&#13;&#10;  ON (PROJ_ALL.PROJECT_TYPE = PROJ_TYPES.PROJECT_TYPE&#13;&#10;  AND PROJ_ALL.ORG_ID       = PROJ_TYPES.ORG_ID)&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,TASK_ID FROM PGSCHARLIE.PA_TASKS) PA_TASK&#13;&#10;  ON ( REV.PROJECT_ID = PA_TASK.PROJECT_ID&#13;&#10;  AND REV.TASK_ID = PA_TASK.TASK_ID )&#13;&#10;  JOIN&#13;&#10;  (SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS &#13;&#10;  WHERE UPPER(NAME) LIKE 'OU%') OU&#13;&#10;  ON&#13;&#10;  PROJ_ALL.ORG_ID = OU.ORGANIZATION_ID&#13;&#10;  JOIN&#13;&#10;  (SELECT TASK_ID,TASK_NUMBER FROM PGSCHARLIE.PA_TASKS) PTSK&#13;&#10;  ON REV.TASK_ID = PTSK.TASK_ID &#13;&#10;  GROUP BY &#13;&#10;  REV.PROJECT_ID,&#13;&#10;  REV.TASK_ID,&#13;&#10;  PTSK.TASK_NUMBER,&#13;&#10;  PROJ_TYPES.PROJECT_TYPE&#13;&#10;  ) AA&#13;&#10;  GROUP BY &#13;&#10;  AA.PROJECT_ID,&#13;&#10;  AA.TASK_ID,&#13;&#10;  AA.TASK_NUMBER,&#13;&#10;  AA.PROJECT_TYPE&#13;&#10;ORDER BY&#13;&#10;AA.TASK_NUMBER&#13;&#10;) BILL_MAIN&#13;&#10;LEFT JOIN&#13;&#10;(&#13;&#10;SELECT &#13;&#10;        I.PROJECT_ID PROJECT_ID,&#13;&#10;        I.TASK_ID TASK_ID,&#13;&#10;        PTSK.TASK_NUMBER,&#13;&#10;        SUM(I.PROJFUNC_BILL_AMOUNT) bill_period_functional_bill,&#13;&#10;        SUM(I.INV_AMOUNT)  bill_period_funding_bill&#13;&#10;  FROM &#13;&#10;  (select ACCTD_CURR_CODE,INV_CURRENCY_CODE,INVOICE_DATE,AGREEMENT_ID,&#13;&#10;  RA_INVOICE_NUMBER,ACCTD_EXCHG_RATE,TRANSFER_STATUS_CODE,&#13;&#10;  SYSTEM_REFERENCE,DRAFT_INVOICE_NUM,DRAFT_INVOICE_NUM_CREDITED,&#13;&#10;  CREATION_DATE,GENERATION_ERROR_FLAG,TRANSFER_REJECTION_REASON,&#13;&#10;  GL_DATE,BILL_THROUGH_DATE,PROJECT_ID,unearned_code_combination_id,&#13;&#10;  unbilled_code_combination_id from pgscharlie.PA_DRAFT_INVOICES_ALL&#13;&#10;  WHERE UPPER(TRANSFER_STATUS_CODE) = 'A') IA&#13;&#10;  join &#13;&#10;  (select PROJECT_ID,TASK_ID,LINE_NUM,TEXT,INV_AMOUNT,EVENT_NUM,&#13;&#10;  FUNDING_BILL_AMOUNT,PROJFUNC_BILL_AMOUNT,DRAFT_INVOICE_NUM &#13;&#10;  from pgscharlie.PA_DRAFT_INVOICE_ITEMS) I&#13;&#10;  on IA.PROJECT_ID = I.PROJECT_ID&#13;&#10;  and IA.DRAFT_INVOICE_NUM = I.DRAFT_INVOICE_NUM&#13;&#10;  join&#13;&#10;  (select segment2,code_combination_id from pgscharlie.gl_code_combinations) gl&#13;&#10;  on coalesce(IA.unearned_code_combination_id,&#10; IA.unbilled_code_combination_id) =  gl.code_combination_id&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,SEGMENT1,PROJECT_TYPE,ORG_ID FROM PGSCHARLIE.PA_PROJECTS_ALL &#13;&#10;  --WHERE SEGMENT1 = '10186211'&#13;&#10;  ) PROJ_ALL&#13;&#10;  ON (IA.PROJECT_ID = PROJ_ALL.PROJECT_ID)&#9;&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_TYPE,ORG_ID FROM PGSCHARLIE.PA_PROJECT_TYPES_ALL &#13;&#10;  WHERE UPPER(ATTRIBUTE10) = 'N') PROJ_TYPES&#13;&#10;  ON (PROJ_ALL.PROJECT_TYPE = PROJ_TYPES.PROJECT_TYPE&#13;&#10;  AND PROJ_ALL.ORG_ID       = PROJ_TYPES.ORG_ID)&#13;&#10;  JOIN&#13;&#10;  (SELECT TASK_ID,TASK_NUMBER FROM PGSCHARLIE.PA_TASKS) PTSK&#13;&#10;  ON I.TASK_ID = PTSK.TASK_ID&#13;&#10;  GROUP BY &#13;&#10;  I.PROJECT_ID,&#13;&#10;  I.TASK_ID,&#13;&#10;  PTSK.TASK_NUMBER&#13;&#10;  UNION ALL&#13;&#10;  SELECT &#13;&#10;  REV.PROJECT_ID,&#13;&#10;  REV.TASK_ID,&#13;&#10;  PTSK.TASK_NUMBER,&#13;&#10;  SUM(REV.PROJFUNC_REVENUE_AMOUNT) bill_period_functional_bill,&#13;&#10;  SUM(REV.PROJFUNC_REVENUE_AMOUNT) bill_period_funding_bill&#13;&#10;  FROM &#13;&#10;  (SELECT PROJECT_ID,TASK_ID,DRAFT_REVENUE_NUM,PROJFUNC_REVENUE_AMOUNT &#13;&#10;  FROM PGSCHARLIE.PA_DRAFT_REVENUE_ITEMS&#13;&#10;   WHERE UPPER(REVENUE_SOURCE) &lt;&gt; 'DUMMY') REV&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,DRAFT_REVENUE_NUM FROM PGSCHARLIE.PA_DRAFT_REVENUES_ALL &#13;&#10;  WHERE UPPER(TRANSFER_STATUS_CODE) = 'A') REV_ALL&#13;&#10;  ON (REV.PROJECT_ID        = REV_ALL.PROJECT_ID&#13;&#10;  AND REV.DRAFT_REVENUE_NUM = REV_ALL.DRAFT_REVENUE_NUM)&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,ORG_ID,PROJECT_TYPE FROM PGSCHARLIE.PA_PROJECTS_ALL &#13;&#10;  ) PROJ_ALL&#13;&#10;  ON (REV.PROJECT_ID = PROJ_ALL.PROJECT_ID)&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_TYPE,ORG_ID FROM PGSCHARLIE.PA_PROJECT_TYPES_ALL &#13;&#10;  WHERE UPPER(ATTRIBUTE10) = 'Y') PROJ_TYPES&#13;&#10;  ON (PROJ_ALL.PROJECT_TYPE = PROJ_TYPES.PROJECT_TYPE&#13;&#10;  AND PROJ_ALL.ORG_ID       = PROJ_TYPES.ORG_ID)&#13;&#10;  JOIN &#13;&#10;  (SELECT PROJECT_ID,TASK_ID FROM PGSCHARLIE.PA_TASKS) PA_TASK&#13;&#10;  ON ( REV.PROJECT_ID = PA_TASK.PROJECT_ID&#13;&#10;  AND REV.TASK_ID = PA_TASK.TASK_ID )&#13;&#10;  JOIN&#13;&#10;  (SELECT TASK_ID,TASK_NUMBER FROM PGSCHARLIE.PA_TASKS) PTSK&#13;&#10;  ON REV.TASK_ID = PTSK.TASK_ID &#13;&#10;  GROUP BY &#13;&#10;  REV.PROJECT_ID,&#13;&#10;  REV.TASK_ID,&#13;&#10;  PTSK.TASK_NUMBER&#13;&#10; ) bill_select_period&#13;&#10; ON&#13;&#10; bill_main.project_ID = bill_select_period.project_ID  &#13;&#10; AND bill_main.task_ID  = bill_select_period.task_ID&#13;&#10;) bill_main2&#13;&#10;ON&#13;&#10;PROJ_FUND.project_id = bill_main2.project_id&#13;&#10;AND PROJ_FUND.task_id = bill_main2.task_id &#13;&#10;) BILL_FINAL        &#13;&#10;LEFT JOIN &#13;&#10;(SELECT PROJECT_ID,SEGMENT1,ORG_ID,PROJECT_TYPE,DESCRIPTION &#13;&#10;FROM PGSCHARLIE.PA_PROJECTS_ALL) PROJ_ALL&#13;&#10;ON (BILL_FINAL.PROJECT_ID = PROJ_ALL.PROJECT_ID)&#9;&#13;&#10;LEFT JOIN&#13;&#10;(SELECT TASK_ID,TASK_NUMBER,ATTRIBUTE1,ATTRIBUTE2,ATTRIBUTE3,ATTRIBUTE6,&#13;&#10;COMPLETION_DATE,DESCRIPTION FROM PGSCHARLIE.PA_TASKS) PTSK&#13;&#10;ON BILL_FINAL.TASK_ID = PTSK.TASK_ID&#13;&#10;LEFT JOIN&#13;&#10;(SELECT DISTINCT PRJ_CLS.CLASS_CODE,PRJ_CLS.PROJECT_ID PROJECT_ID &#13;&#10;FROM PGSCHARLIE.PA_PROJECT_CLASSES PRJ_CLS  &#13;&#10;WHERE upper(CLASS_CATEGORY) = 'SEGMENT') PROJ_CLS&#13;&#10;ON ( PROJ_ALL.PROJECT_ID = PROJ_CLS.PROJECT_ID )&#13;&#10;LEFT JOIN&#13;&#10;(SELECT DISTINCT L.ATTRIBUTE4 AS PROJ_LE_ME,&#13;&#10;P.PROJECT_ID                AS PROJECT_ID&#13;&#10;FROM &#13;&#10;PGSCHARLIE.FND_LOOKUP_VALUES L&#13;&#10;JOIN &#13;&#10;PGSCHARLIE.PA_PROJECT_CLASSES P&#13;&#10;ON (L.DESCRIPTION = P.CLASS_CODE)&#13;&#10;JOIN &#13;&#10;PGSCHARLIE.PA_PROJECTS_ALL PM&#13;&#10;ON (P.PROJECT_ID = PM.PROJECT_ID)&#13;&#10;JOIN &#13;&#10;PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS OM&#13;&#10;ON (PM.CARRYING_OUT_ORGANIZATION_ID      = OM.ORGANIZATION_ID&#13;&#10;AND L.TAG                                = OM.NAME)&#13;&#10;WHERE &#13;&#10;UPPER(L.LOOKUP_TYPE)               = 'PS AR RELATION SEARCH'&#13;&#10;AND UPPER(P.CLASS_CATEGORY)              = 'SEGMENT'&#13;&#10;) PROJ_LE_ME_VAL &#13;&#10;ON PROJ_ALL.PROJECT_ID = PROJ_LE_ME_VAL.PROJECT_ID&#13;&#10;LEFT JOIN&#13;&#10;(SELECT PROJECT_TYPE,ORG_ID,ATTRIBUTE7 FROM PGSCHARLIE.PA_PROJECT_TYPES_ALL) TYPES_ALL&#13;&#10;ON (PROJ_ALL.PROJECT_TYPE = TYPES_ALL.PROJECT_TYPE&#13;&#10;AND PROJ_ALL.ORG_ID        =TYPES_ALL.ORG_ID ) &#13;&#10;LEFT JOIN&#13;&#10;(SELECT &#13;&#10;DISTINCT P.PROJECT_ID AS PROJECT_ID,&#13;&#10;MAX(F.FULL_NAME)            AS FULL_NAME&#13;&#10;FROM&#13;&#10;        (SELECT PPP.PROJECT_ID,&#13;&#10;          PPP.RESOURCE_SOURCE_ID PERSON_ID,&#13;&#10;        COALESCE(PPP.START_DATE_ACTIVE, TO_DATE('01/01/1900', 'MM DD YYYY')) START_DATE_ACTIVE&#13;&#10;        FROM &#13;&#10;        (SELECT PROJECT_ID,PROJECT_ROLE_ID,OBJECT_TYPE,RESOURCE_TYPE_ID,&#13;&#10;&#9;&#9;RESOURCE_SOURCE_ID,START_DATE_ACTIVE &#13;&#10;        FROM PGSCHARLIE.PA_PROJECT_PARTIES) PPP&#13;&#10;        INNER JOIN &#13;&#10;        (SELECT PROJECT_ROLE_ID,ROLE_CONTROL_CODE FROM PGSCHARLIE.PA_ROLE_CONTROLS) PRC&#13;&#10;        ON ( PPP.PROJECT_ROLE_ID = PRC.PROJECT_ROLE_ID )&#13;&#10;        INNER JOIN &#13;&#10;        (SELECT PROJECT_ROLE_TYPE,PROJECT_ROLE_ID FROM PGSCHARLIE.PA_PROJECT_ROLE_TYPES_B) PPRT&#13;&#10;        ON ( PPP.PROJECT_ROLE_ID         = PPRT.PROJECT_ROLE_ID )&#13;&#10;        WHERE &#13;&#10;        PPRT.PROJECT_ROLE_TYPE     = '1003'&#13;&#10;        AND UPPER(PPP.OBJECT_TYPE)       = 'PA_PROJECTS'&#13;&#10;        AND UPPER(PRC.ROLE_CONTROL_CODE) = 'ALLOW_AS_PROJ_MEMBER'&#13;&#10;        AND PPP.RESOURCE_TYPE_ID         = 101&#13;&#10;        ) P&#13;&#10;      INNER JOIN &#13;&#10;      (SELECT PERSON_ID,CAST(EFFECTIVE_END_DATE AS DATE) EFFECTIVE_END_DATE,FULL_NAME &#13;&#10;&#9;  FROM PGSCHARLIE.PER_ALL_PEOPLE_F) F&#13;&#10;      ON ( P.PERSON_ID = F.PERSON_ID )&#13;&#10;      INNER JOIN&#13;&#10;      (SELECT AP.PERSON_ID,&#13;&#10;          MAX(CAST(AP.EFFECTIVE_END_DATE AS DATE)) EFFECTIVE_END_DATE&#13;&#10;        FROM PGSCHARLIE.PER_ALL_PEOPLE_F AP&#13;&#10;        GROUP BY AP.PERSON_ID&#13;&#10;      ) FF&#13;&#10;      ON F.PERSON_ID         = FF.PERSON_ID&#13;&#10;      AND F.EFFECTIVE_END_DATE = FF.EFFECTIVE_END_DATE &#13;&#10;      INNER JOIN&#13;&#10;        (SELECT PPP.PROJECT_ID,&#13;&#10;          COALESCE(MAX(PPP.START_DATE_ACTIVE), TO_DATE('01/01/1900',&#10;  'MM DD YYYY')) START_DATE_ACTIVE&#13;&#10;        FROM &#13;&#10;        (SELECT PROJECT_ID,START_DATE_ACTIVE,PROJECT_ROLE_ID,OBJECT_TYPE,RESOURCE_TYPE_ID &#13;&#10;&#9;&#9;FROM PGSCHARLIE.PA_PROJECT_PARTIES) PPP&#13;&#10;        INNER JOIN &#13;&#10;        (SELECT PROJECT_ROLE_ID,ROLE_CONTROL_CODE FROM PGSCHARLIE.PA_ROLE_CONTROLS) PRC&#13;&#10;        ON ( PPP.PROJECT_ROLE_ID = PRC.PROJECT_ROLE_ID )&#13;&#10;        INNER JOIN &#13;&#10;        PGSCHARLIE.PA_PROJECT_ROLE_TYPES_B PPRT&#13;&#10;        ON ( PPP.PROJECT_ROLE_ID         = PPRT.PROJECT_ROLE_ID )&#13;&#10;        WHERE PPRT.PROJECT_ROLE_TYPE     = '1003'&#13;&#10;        AND UPPER(PPP.OBJECT_TYPE)       = 'PA_PROJECTS'&#13;&#10;        AND UPPER(PRC.ROLE_CONTROL_CODE) = 'ALLOW_AS_PROJ_MEMBER'&#13;&#10;        AND PPP.RESOURCE_TYPE_ID         = 101&#13;&#10;        GROUP BY PPP.PROJECT_ID&#13;&#10;        ) PP &#13;&#10;        ON ( P.PROJECT_ID = PP.PROJECT_ID&#13;&#10;      AND P.START_DATE_ACTIVE  = PP.START_DATE_ACTIVE )&#13;&#10;      GROUP BY P.PROJECT_ID&#13;&#10;      ) PROJ_SRVC_MGR &#13;&#10;ON PROJ_ALL.PROJECT_ID = PROJ_SRVC_MGR.PROJECT_ID&#13;&#10;WHERE&#13;&#10;(PROJ_CLS.CLASS_CODE like '%Core Maintenance%'  &#13;&#10;OR PROJ_CLS.CLASS_CODE like '%TDI%' &#13;&#10;OR PROJ_CLS.CLASS_CODE like 'TX Repairs%' &#13;&#10;OR PROJ_CLS.CLASS_CODE like 'TX Parts%' &#13;&#10;OR PROJ_LE_ME_VAL.PROJ_LE_ME like '%06' &#13;&#10;) &#13;&#10;AND UPPER(PROJ_CLS.CLASS_CODE) NOT LIKE '%SHADOW%' &#13;&#10;AND (REPLACE(PROJ_ALL.PROJECT_TYPE,'&amp;','_') not like '%T_M%' OR PROJ_ALL.SEGMENT1 in ('10189605'))&#13;&#10;) FACT_VALUES&#13;&#10;JOIN&#13;&#10;  (&#13;&#10;  SELECT DISTINCT FUND.PROJECT_ID,&#13;&#10;    FUND.TASK_ID ,&#13;&#10;    PROJ_MAIN.PROJECT_STATUS_CODE PROJ_STAT_VAL,&#13;&#10;    PROJ_MAIN.ATTRIBUTE5 PROJ_EVNT_ID,&#13;&#10;    PROJ_MAIN.PROJ_CRRCY_CD,&#13;&#10;    PROJ_MAIN.EMPLY_FULL_NM,&#13;&#10;    PROJ_PL.FULL_NAME PROJ_PL_NM,&#13;&#10;    CUSTOMER_DET.CUSTOMER_NAME cust_nm,&#13;&#10;    CASE&#13;&#10;      WHEN CUSTOMER_DET.CUSTOMER_TYPE = 'I'&#13;&#10;      THEN 'Internal'&#13;&#10;      WHEN CUSTOMER_DET.CUSTOMER_TYPE = 'R'&#13;&#10;      THEN 'External'&#13;&#10;      ELSE 'NA'&#13;&#10;    END CUST_TYPE_NM,&#13;&#10;    MAX(PROJECT_CLASS_CODES.CLASS_CODE) PROJ_CLSS_DESC,&#13;&#10;    CUSTOMER_SHIPPED_DET.CUSTOMER_SHIPPED_NAME CUSTOMER_SHIPPED_TO_NAME,&#13;&#10;    loc_det.COUNTRY CUST_ADDR_CNTRY_NM,&#13;&#10;    loc_det.STATE CUST_ADDR_STATE_NM,&#13;&#10;    PROJ_MAIN.ORG_NM,&#13;&#10;    PROJ_MAIN.PARENT_ORG_NM    &#13;&#10;  FROM &#13;&#10;  (select PROJECT_ID,TASK_ID from PGSCHARLIE.PA_PROJECT_FUNDINGS) AS FUND&#13;&#10;  JOIN&#13;&#10;    (SELECT PROJ_ALL.PROJECT_ID,&#13;&#10;      proj_all.PROJECT_STATUS_CODE,&#13;&#10;      proj_all.ATTRIBUTE5,&#13;&#10;      proj_all.PROJECT_CURRENCY_CODE PROJ_CRRCY_CD,&#13;&#10;      PROJ_SRVC_MGR.FULL_NAME EMPLY_FULL_NM,&#13;&#10;      OU.NAME ORG_NM,&#13;&#10;      (CASE WHEN CAST(OU.BUSINESS_GROUP_ID AS NUMERIC(15,0)) IS NULL THEN &#13;&#10;&#9;&#9;(CASE WHEN CAST(LE.LE_NM AS NUMERIC(15,0)) IS NULL THEN &#13;&#10;&#9;&#9;&#9;(CASE WHEN CAST(LE1.OPERATING_UNIT AS NUMERIC(15,0)) IS NOT NULL &#13;&#10;&#9;&#9;&#9;THEN CAST(LE1.OPERATING_UNIT AS NUMERIC(15,0))  &#13;&#10;&#9;&#9;&#9;ELSE 0 END)&#13;&#10;&#9;&#9;ELSE&#13;&#10;&#9;&#9;CAST(LE.LE_NM AS NUMERIC(15,0)) END)&#13;&#10;&#9;ELSE&#13;&#10;&#9;CAST(OU.BUSINESS_GROUP_ID AS NUMERIC(15,0))&#13;&#10;&#9;END) PARENT_ORG_NM&#13;&#10;    FROM &#13;&#10;    (select PROJECT_ID,PROJECT_STATUS_CODE,ATTRIBUTE5,PROJECT_CURRENCY_CODE,ORG_ID &#13;&#10;&#9;from PGSCHARLIE.PA_PROJECTS_ALL) PROJ_ALL&#13;&#10;    JOIN&#13;&#10;      (SELECT DISTINCT PPP.PROJECT_ID AS PROJECT_ID,&#13;&#10;        MAX(F.FULL_NAME)              AS FULL_NAME&#13;&#10;      FROM &#13;&#10;      (select PROJECT_ID,RESOURCE_TYPE_ID,PROJECT_ROLE_ID,RESOURCE_SOURCE_ID,OBJECT_TYPE &#13;&#10;&#9;  from PGSCHARLIE.PA_PROJECT_PARTIES) PPP&#13;&#10;      INNER JOIN &#13;&#10;      (select PROJECT_ROLE_ID,ROLE_CONTROL_CODE from PGSCHARLIE.PA_ROLE_CONTROLS) PRC&#13;&#10;      ON ( PPP.PROJECT_ROLE_ID = PRC.PROJECT_ROLE_ID )&#13;&#10;      INNER JOIN &#13;&#10;      (select PROJECT_ROLE_ID,PROJECT_ROLE_TYPE from PGSCHARLIE.PA_PROJECT_ROLE_TYPES_B) PPRT&#13;&#10;      ON ( PPP.PROJECT_ROLE_ID = PPRT.PROJECT_ROLE_ID )&#13;&#10;      INNER JOIN &#13;&#10;      (select FULL_NAME,PERSON_ID from PGSCHARLIE.PER_ALL_PEOPLE_F) F&#13;&#10;      ON ( PPP.RESOURCE_SOURCE_ID      = F.PERSON_ID )&#13;&#10;      WHERE PPRT.PROJECT_ROLE_TYPE     = '1003'&#13;&#10;      AND UPPER(PPP.OBJECT_TYPE)       = 'PA_PROJECTS'&#13;&#10;      AND UPPER(PRC.ROLE_CONTROL_CODE) = 'ALLOW_AS_PROJ_MEMBER'&#13;&#10;      AND PPP.RESOURCE_TYPE_ID         = 101&#13;&#10;      GROUP BY PPP.PROJECT_ID&#13;&#10;      ) PROJ_SRVC_MGR &#13;&#10;      ON ( PROJ_ALL.PROJECT_ID = PROJ_SRVC_MGR.PROJECT_ID )&#13;&#10;      JOIN&#13;&#10;      (SELECT ORGANIZATION_ID,NAME,BUSINESS_GROUP_ID &#13;&#10;&#9;  FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS WHERE UPPER(NAME) LIKE 'OU%') OU&#13;&#10;       ON&#13;&#10;       PROJ_ALL.ORG_ID = OU.ORGANIZATION_ID&#13;&#10;       LEFT JOIN&#13;&#10;       (&#13;&#10;        select &#13;&#10;&#9;DISTINCT &#13;&#10;&#9;P.LE as LE_NM, P.OPERATING_UNIT as OPERATING_UNIT&#13;&#10;&#9;from&#13;&#10;&#9;(SELECT OU1.BUSINESS_GROUP_ID, OU2.NAME BUSINESS_GROUP_NAME, &#13;&#10;&#9;OI2.ORG_INFORMATION2 LE, OU3.NAME LEGAL_ENTITY, &#13;&#10;&#9;OU1.ORGANIZATION_ID OPERATING_UNIT, OU1.NAME OU_NAME&#13;&#10;&#9;FROM&#13;&#10;&#9;(SELECT BUSINESS_GROUP_ID,ORGANIZATION_ID,NAME &#13;&#10;&#9;FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU1,&#13;&#10;&#9;(SELECT NAME,ORGANIZATION_ID FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU2,&#13;&#10;&#9;(SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU3,&#13;&#10;&#9;(SELECT ORGANIZATION_ID,ORG_INFORMATION_CONTEXT,ORG_INFORMATION1,ORG_INFORMATION2 &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI1,&#13;&#10;&#9;(SELECT ORG_INFORMATION2,ORGANIZATION_ID,ORG_INFORMATION_CONTEXT &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI2&#13;&#10;&#9;WHERE &#13;&#10;&#9;OU1.ORGANIZATION_ID = OI1.ORGANIZATION_ID &#13;&#10;&#9;AND OU1.ORGANIZATION_ID = OI2.ORGANIZATION_ID &#13;&#10;&#9;AND OU1.BUSINESS_GROUP_ID = OU2.ORGANIZATION_ID &#13;&#10;&#9;AND OI2.ORG_INFORMATION2 = OU3.ORGANIZATION_ID &#13;&#10;&#9;AND OI1.ORG_INFORMATION_CONTEXT = 'CLASS' AND OI1.ORG_INFORMATION1 = 'OPERATING_UNIT' &#13;&#10;&#9;AND OI1.ORG_INFORMATION2 = 'Y' AND OI2.ORG_INFORMATION_CONTEXT = 'Operating Unit Information') P&#13;&#10;&#9;LEFT JOIN&#13;&#10;&#9;(&#13;&#10;&#9;(select OU.ORGANIZATION_ID, OU.NAME, OI.ORG_INFORMATION1 OU&#13;&#10;&#9;from &#13;&#10;&#9;(SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU, &#13;&#10;&#9;(SELECT ORGANIZATION_ID,ORG_INFORMATION_CONTEXT,ORG_INFORMATION1 &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI&#13;&#10;&#9;where OU.ORGANIZATION_ID = OI.ORGANIZATION_ID and OI.ORG_INFORMATION_CONTEXT = 'Exp Organization &#10; Defaults')&#13;&#10;&#9;union&#13;&#10;&#9;(&#13;&#10;&#9;select OU.ORGANIZATION_ID, &#13;&#10;&#9;OU.NAME, &#13;&#10;&#9;OI.ORG_INFORMATION3 OU&#13;&#10;&#9;from &#13;&#10;&#9;(SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU, &#13;&#10;&#9;(SELECT ORG_INFORMATION3,ORGANIZATION_ID,ORG_INFORMATION_CONTEXT &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI&#13;&#10;&#9;where &#13;&#10;&#9;OU.ORGANIZATION_ID = OI.ORGANIZATION_ID &#13;&#10;&#9;and OI.ORG_INFORMATION_CONTEXT = 'Accounting Information')&#13;&#10;&#9;) Q&#13;&#10;&#9;ON P.OPERATING_UNIT = Q.OU&#13;&#10;&#9;) LE&#13;&#10;&#9;ON&#13;&#10;&#9;OU.ORGANIZATION_ID = LE.OPERATING_UNIT&#13;&#10;&#9;LEFT JOIN&#13;&#10;       (&#13;&#10;        select &#13;&#10;&#9;DISTINCT &#13;&#10;&#9;P.LE as LE_NM, &#13;&#10;&#9;P.OPERATING_UNIT as OPERATING_UNIT,&#13;&#10;&#9;Q.ORGANIZATION_ID as ORGANIZATION_ID&#13;&#10;&#9;from&#13;&#10;&#9;(SELECT OU1.BUSINESS_GROUP_ID, OU2.NAME BUSINESS_GROUP_NAME, &#13;&#10;&#9;OI2.ORG_INFORMATION2 LE, OU3.NAME LEGAL_ENTITY, &#13;&#10;&#9;OU1.ORGANIZATION_ID OPERATING_UNIT, OU1.NAME OU_NAME&#13;&#10;&#9;FROM&#13;&#10;&#9;(SELECT BUSINESS_GROUP_ID,ORGANIZATION_ID,NAME &#13;&#10;&#9;FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU1,&#13;&#10;&#9;(SELECT NAME,ORGANIZATION_ID FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU2,&#13;&#10;&#9;(SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU3,&#13;&#10;&#9;(SELECT ORGANIZATION_ID,ORG_INFORMATION_CONTEXT,ORG_INFORMATION1,ORG_INFORMATION2 &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI1,&#13;&#10;&#9;(SELECT ORG_INFORMATION2,ORGANIZATION_ID,ORG_INFORMATION_CONTEXT &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI2&#13;&#10;&#9;WHERE &#13;&#10;&#9;OU1.ORGANIZATION_ID = OI1.ORGANIZATION_ID &#13;&#10;&#9;AND OU1.ORGANIZATION_ID = OI2.ORGANIZATION_ID &#13;&#10;&#9;AND OU1.BUSINESS_GROUP_ID = OU2.ORGANIZATION_ID &#13;&#10;&#9;AND OI2.ORG_INFORMATION2 = OU3.ORGANIZATION_ID &#13;&#10;&#9;AND OI1.ORG_INFORMATION_CONTEXT = 'CLASS' AND OI1.ORG_INFORMATION1 = 'OPERATING_UNIT' &#13;&#10;&#9;AND OI1.ORG_INFORMATION2 = 'Y' &#13;&#10;&#9;AND OI2.ORG_INFORMATION_CONTEXT = 'Operating Unit Information') P&#13;&#10;&#9;LEFT JOIN&#13;&#10;&#9;(&#13;&#10;&#9;(select OU.ORGANIZATION_ID, OU.NAME, OI.ORG_INFORMATION1 OU&#13;&#10;&#9;from &#13;&#10;&#9;(SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU, &#13;&#10;&#9;(SELECT ORGANIZATION_ID,ORG_INFORMATION_CONTEXT,ORG_INFORMATION1 &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI&#13;&#10;&#9;where OU.ORGANIZATION_ID = OI.ORGANIZATION_ID &#13;&#10;&#9;and OI.ORG_INFORMATION_CONTEXT = 'Exp Organization Defaults')&#13;&#10;&#9;union&#13;&#10;&#9;(&#13;&#10;&#9;select OU.ORGANIZATION_ID, &#13;&#10;&#9;OU.NAME, &#13;&#10;&#9;OI.ORG_INFORMATION3 OU&#13;&#10;&#9;from &#13;&#10;&#9;(SELECT ORGANIZATION_ID,NAME FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS) OU, &#13;&#10;&#9;(SELECT ORG_INFORMATION3,ORGANIZATION_ID,ORG_INFORMATION_CONTEXT &#13;&#10;&#9;FROM PGSCHARLIE.HR_ORGANIZATION_INFORMATION) OI&#13;&#10;&#9;where &#13;&#10;&#9;OU.ORGANIZATION_ID = OI.ORGANIZATION_ID &#13;&#10;&#9;and OI.ORG_INFORMATION_CONTEXT = 'Accounting Information')&#13;&#10;&#9;) Q&#13;&#10;&#9;ON P.OPERATING_UNIT = Q.OU&#13;&#10;&#9;) LE1&#13;&#10;&#9;ON&#13;&#10;&#9;OU.ORGANIZATION_ID = LE1.ORGANIZATION_ID&#13;&#10;    ) PROJ_MAIN &#13;&#10;    ON (FUND.PROJECT_ID             =PROJ_MAIN.PROJECT_ID)&#13;&#10;     JOIN&#13;&#10;    (SELECT PPP.PROJECT_ID AS PROJECT_ID,&#13;&#10;      MAX(F.FULL_NAME)     AS FULL_NAME&#13;&#10;    FROM &#13;&#10;    (select PROJECT_ID,RESOURCE_TYPE_ID,OBJECT_TYPE,PROJECT_ROLE_ID,RESOURCE_SOURCE_ID &#13;&#10;&#9;from PGSCHARLIE.PA_PROJECT_PARTIES) PPP&#13;&#10;    INNER JOIN &#13;&#10;    (select PROJECT_ROLE_ID,ROLE_CONTROL_CODE from PGSCHARLIE.PA_ROLE_CONTROLS) PRC&#13;&#10;    ON ( PPP.PROJECT_ROLE_ID = PRC.PROJECT_ROLE_ID )&#13;&#10;    INNER JOIN &#13;&#10;    (select PROJECT_ROLE_ID,PROJECT_ROLE_TYPE from PGSCHARLIE.PA_PROJECT_ROLE_TYPES_B) PPRT&#13;&#10;    ON ( PPP.PROJECT_ROLE_ID = PPRT.PROJECT_ROLE_ID )&#13;&#10;    JOIN &#13;&#10;    (select FULL_NAME,PERSON_ID from PGSCHARLIE.PER_ALL_PEOPLE_F) F&#13;&#10;    ON ( PPP.RESOURCE_SOURCE_ID  = F.PERSON_ID )&#13;&#10;    WHERE PPRT.PROJECT_ROLE_TYPE = 'PROJECT MANAGER'&#13;&#10;    AND PPP.OBJECT_TYPE          = 'PA_PROJECTS'&#13;&#10;    AND PRC.ROLE_CONTROL_CODE    = 'ALLOW_AS_PROJ_MEMBER'&#13;&#10;    AND PPP.RESOURCE_TYPE_ID     = 101&#13;&#10;    GROUP BY PPP.PROJECT_ID&#13;&#10;    ) PROJ_PL ON ( PROJ_MAIN.PROJECT_ID = PROJ_PL.PROJECT_ID )&#13;&#10;  JOIN&#13;&#10;    (SELECT a.PROJECT_ID PROJECT_ID,&#13;&#10;      COALESCE(b.ACCOUNT_NAME,c.PARTY_NAME) CUSTOMER_NAME,&#13;&#10;      b.CUSTOMER_TYPE&#13;&#10;    FROM &#13;&#10;    (select PROJECT_ID,BILL_TO_CUSTOMER_ID,SHIP_TO_CUSTOMER_ID,DEFAULT_TOP_TASK_CUST_FLAG &#13;&#10;&#9;from PGSCHARLIE.PA_PROJECT_CUSTOMERS) a&#13;&#10;    LEFT OUTER JOIN &#13;&#10;    (select ACCOUNT_NAME,CUSTOMER_TYPE,CUST_ACCOUNT_ID,&#10; PARTY_ID from PGSCHARLIE.HZ_CUST_ACCOUNTS) b&#13;&#10;    ON ( a.BILL_TO_CUSTOMER_ID =b.CUST_ACCOUNT_ID )&#13;&#10;    LEFT OUTER JOIN &#13;&#10;    (select PARTY_NAME,PARTY_ID from PGSCHARLIE.HZ_PARTIES) c&#13;&#10;    ON ( b.PARTY_ID                          =c.PARTY_ID )&#13;&#10;    WHERE a.DEFAULT_TOP_TASK_CUST_FLAG       = 'Y'&#13;&#10;    ) CUSTOMER_DET ON ( PROJ_MAIN.PROJECT_ID = CUSTOMER_DET.PROJECT_ID )&#13;&#10;  JOIN&#13;&#10;    (SELECT DISTINCT PC.PROJECT_ID AS PROJECT_ID ,&#13;&#10;      PC.CLASS_CODE&#13;&#10;    FROM &#13;&#10;    PGSCHARLIE.PA_PROJECT_CLASSES PC&#13;&#10;    WHERE PC.CLASS_CATEGORY = 'Segment'&#13;&#10;    ) PROJECT_CLASS_CODES&#13;&#10;  ON ( PROJ_MAIN.PROJECT_ID = PROJECT_CLASS_CODES.PROJECT_ID )&#13;&#10;  JOIN&#13;&#10;    (SELECT a.PROJECT_ID PROJECT_ID,&#13;&#10;      COALESCE(b.ACCOUNT_NAME,c.PARTY_NAME) CUSTOMER_SHIPPED_NAME,&#13;&#10;      b.CUSTOMER_TYPE&#13;&#10;    FROM &#13;&#10;    (select PROJECT_ID,SHIP_TO_CUSTOMER_ID,DEFAULT_TOP_TASK_CUST_FLAG &#13;&#10;&#9;from PGSCHARLIE.PA_PROJECT_CUSTOMERS) a&#13;&#10;    LEFT OUTER JOIN &#13;&#10;    (select ACCOUNT_NAME,CUSTOMER_TYPE,CUST_ACCOUNT_ID,PARTY_ID &#13;&#10;&#9;from PGSCHARLIE.HZ_CUST_ACCOUNTS) b&#13;&#10;    ON ( a.SHIP_TO_CUSTOMER_ID =b.CUST_ACCOUNT_ID )&#13;&#10;    LEFT OUTER JOIN &#13;&#10;    (select PARTY_NAME,PARTY_ID from PGSCHARLIE.HZ_PARTIES) c&#13;&#10;    ON ( b.PARTY_ID                                  =c.PARTY_ID )&#13;&#10;    WHERE a.DEFAULT_TOP_TASK_CUST_FLAG               = 'Y'&#13;&#10;    ) CUSTOMER_SHIPPED_DET ON ( PROJ_MAIN.PROJECT_ID = CUSTOMER_SHIPPED_DET.PROJECT_ID )&#13;&#10;  JOIN&#13;&#10;    (SELECT hzl.COUNTRY,&#13;&#10;      hzl.STATE,&#13;&#10;      PPC.PROJECT_ID PROJECT_ID&#13;&#10;    FROM &#13;&#10;    (select PROJECT_ID,SHIP_TO_ADDRESS_ID,DEFAULT_TOP_TASK_CUST_FLAG &#13;&#10;&#9;from PGSCHARLIE.PA_PROJECT_CUSTOMERS) PPC&#13;&#10;    INNER JOIN &#13;&#10;    (select CUST_ACCT_SITE_ID,PARTY_SITE_ID from PGSCHARLIE.HZ_CUST_ACCT_SITES_ALL) cas&#13;&#10;    ON ( PPC.SHIP_TO_ADDRESS_ID = cas.CUST_ACCT_SITE_ID )&#13;&#10;    INNER JOIN &#13;&#10;    (select PARTY_SITE_ID,LOCATION_ID from PGSCHARLIE.HZ_PARTY_SITES) hpc&#13;&#10;    ON ( cas.PARTY_SITE_ID =hpc.PARTY_SITE_ID )&#13;&#10;    INNER JOIN &#13;&#10;    (select COUNTRY,STATE,LOCATION_ID from PGSCHARLIE.HZ_LOCATIONS) hzl&#13;&#10;    ON ( hpc.LOCATION_ID                 = hzl.LOCATION_ID )&#13;&#10;    WHERE PPC.DEFAULT_TOP_TASK_CUST_FLAG = 'Y'&#13;&#10;    ) loc_det ON ( PROJ_MAIN.PROJECT_ID  = loc_det.PROJECT_ID )&#13;&#10;  WHERE PROJ_MAIN.PROJECT_STATUS_CODE   IN ('APPROVED', '1000', '1001','1040','1041')&#13;&#10;  GROUP BY FUND.PROJECT_ID,&#13;&#10;    FUND.TASK_ID ,&#13;&#10;    PROJ_MAIN.PROJECT_STATUS_CODE ,&#13;&#10;    PROJ_MAIN.ATTRIBUTE5 ,&#13;&#10;    PROJ_PL.FULL_NAME ,&#13;&#10;    PROJ_MAIN.ORG_NM,&#13;&#10;    proj_main.parent_org_nm,&#13;&#10;    CUSTOMER_DET.CUSTOMER_NAME ,&#13;&#10;    CASE&#13;&#10;      WHEN CUSTOMER_DET.CUSTOMER_TYPE = 'I'&#13;&#10;      THEN 'Internal'&#13;&#10;      WHEN CUSTOMER_DET.CUSTOMER_TYPE = 'R'&#13;&#10;      THEN 'External'&#13;&#10;      ELSE 'NA'&#13;&#10;    END ,&#13;&#10;    PROJ_MAIN.EMPLY_FULL_NM,&#13;&#10;    PROJ_MAIN.PROJ_CRRCY_CD,&#13;&#10;    CUSTOMER_SHIPPED_DET.CUSTOMER_SHIPPED_NAME ,&#13;&#10;    loc_det.COUNTRY ,&#13;&#10;    loc_det.STATE&#13;&#10;  ) OTHER_DIM_ATTRIBUTES &#13;&#10;ON &#13;&#10;FACT_VALUES.PROJECT_ID = OTHER_DIM_ATTRIBUTES.PROJECT_ID&#13;&#10;AND FACT_VALUES.TASK_ID = OTHER_DIM_ATTRIBUTES.TASK_ID&#13;&#10;LEFT OUTER JOIN&#13;&#10;  (SELECT PROJECT_ID,&#13;&#10;    TASK_ID,&#13;&#10;    CALENDAR_FISCL_YR_MO_NUM,&#13;&#10;    BUDGET_FORCST_SALES_AMT,&#13;&#10;    CASE&#13;&#10;      WHEN SUM(BUDGET_FORCST_SALES_AMT) OVER (PARTITION BY PROJECT_ID,TASK_ID ORDER BY 'A')=0&#13;&#10;      THEN 1                      /COUNT(BUDGET_FORCST_SALES_AMT) &#13;&#10;&#9;  OVER (PARTITION BY PROJECT_ID,TASK_ID ORDER BY 'A')&#13;&#10;      ELSE BUDGET_FORCST_SALES_AMT/SUM(BUDGET_FORCST_SALES_AMT) &#13;&#10;&#9;  OVER (PARTITION BY PROJECT_ID,TASK_ID ORDER BY 'A')&#13;&#10;    END PRCTG&#13;&#10;  FROM&#13;&#10;    (SELECT RA.PROJECT_ID,&#13;&#10;      RA.TASK_ID,&#13;&#10;      TO_CHAR(BL.START_DATE,'YYYYMM') CALENDAR_FISCL_YR_MO_NUM,&#13;&#10;      SUM(COALESCE(bl.PROJECT_REVENUE,0)) BUDGET_FORCST_SALES_AMT&#13;&#10;    FROM &#13;&#10;      (select START_DATE,PROJECT_REVENUE,RESOURCE_ASSIGNMENT_ID &#13;&#10;&#9;  from pgscharlie.PA_BUDGET_LINES) BL,&#13;&#10;      (select PROJECT_ID,TASK_ID,RESOURCE_ASSIGNMENT_ID,BUDGET_VERSION_ID &#13;&#10;&#9;  from pgscharlie.PA_RESOURCE_ASSIGNMENTS) RA,&#13;&#10;      (select BUDGET_VERSION_ID,FIN_PLAN_TYPE_ID,BUDGET_STATUS_CODE,CURRENT_FLAG &#13;&#10;&#9;  from pgscharlie.PA_BUDGET_VERSIONS) BV,&#13;&#10;      (select FIN_PLAN_TYPE_ID,name,language from pgscharlie.PA_FIN_PLAN_TYPES_TL) FP&#13;&#10;    WHERE BL.RESOURCE_ASSIGNMENT_ID      = RA.RESOURCE_ASSIGNMENT_ID&#13;&#10;    AND RA.BUDGET_VERSION_ID             = BV.BUDGET_VERSION_ID&#13;&#10;    AND BV.FIN_PLAN_TYPE_ID              = FP.FIN_PLAN_TYPE_ID&#13;&#10;    AND fp.name                          ='PS_IFS_Forecast Revenue Budget'&#13;&#10;    AND fp.language                      ='US'&#13;&#10;    AND bv.BUDGET_STATUS_CODE            ='B'&#13;&#10;    AND bv.CURRENT_FLAG                  ='Y'&#13;&#10;    AND TO_CHAR(BL.START_DATE,'YYYYMM') &gt;= TO_CHAR(CURRENT_DATE,'YYYYMM')&#13;&#10;    GROUP BY RA.PROJECT_ID,&#13;&#10;      RA.TASK_ID,&#13;&#10;      TO_CHAR(BL.START_DATE,'YYYYMM')&#13;&#10;    ) AAAA&#13;&#10;  ) BUDGET_PCTG&#13;&#10;ON ( FACT_VALUES.PROJECT_ID =BUDGET_PCTG.PROJECT_ID&#13;&#10;AND FACT_VALUES.TASK_ID     = BUDGET_PCTG.TASK_ID )&#13;&#10;LEFT JOIN&#13;&#10;(SELECT ORGANIZATION_ID,NAME,BUSINESS_GROUP_ID &#13;&#10;FROM PGSCHARLIE.HR_ALL_ORGANIZATION_UNITS WHERE UPPER(NAME) LIKE 'OU%') OU1&#13;&#10;ON&#13;&#10;OTHER_DIM_ATTRIBUTES.PARENT_ORG_NM = OU1.ORGANIZATION_ID&#13;&#10;) A&#13;&#10;) AA&#13;&#10;where &#13;&#10;(COALESCE(AA.CUST_TYPE_NM,'UNKWN')&lt;&gt;'I' OR AA.PROJ_LE_ME_VAL LIKE '%06')&#13;&#10;AND (AA.BACKLOG_TYPE='R' OR AA.PROJ_LE_ME_VAL LIKE '%06')&#13;&#10;AND AA.FUNC_CURR_BACKLOG_AMT &gt; 0&#13;&#10;AND AA.proj_num not in ('10228751','10209699')&quot;"/>
